<table id="example1" class="table table-bordered table-striped" role="grid" aria-describedby="example1_info">
    <thead>
        <tr role="row">
            <th class="sorting" tabindex="0" aria-controls="example1" rowspan="1" colspan="1"
                aria-label="Nom: active to sort column ascending">Nom</th>
            <th class="sorting" tabindex="0" aria-controls="example1" rowspan="1" colspan="1"
                aria-label="Prenom: active to sort column ascending">Prenom</th>
            <th class="sorting" tabindex="0" aria-controls="example1" rowspan="1" colspan="1"
               aria-label="Date d'enregistrement: active to sort column ascending">Date d'enregistrement</th>
            <th class="sorting" tabindex="0" aria-controls="example1" rowspan="1" colspan="1"
                aria-label="Defendeur: active to sort column ascending">Partie</th>
            <th>Action</th>
        </tr>
    </thead>

    <tbody>
        <tr role="row" class="odd" ng-repeat="individu in individus | limitTo : limit | filter:filter.demandeur">
            <td>{{individu.nom_individu}}</td>
            <td>{{individu.prenom_individu}}</td>
            <td>{{individu.date_creation}}</td>
            <td>{{individu.libelle_typeIndividu}}</td>
            <td>
                <div class="btn-group">
                    <button class="btn btn-default dropdown-toggle" data-toggle="dropdown" aria-expanded="false">Modifier la categorie<span class="caret"></span></button>
                    <ul class="dropdown-menu" role="menu">
                        <li><a href="/formModifIndividu_url/{{individu.id_individu}}">Modifier l'individu</a></li>
                        <li class="divider"></li>
                        <li><a href="/supprimerIndividu_url/{{individu.id_individu}}" class="danger">Supprimer l'individu</a></li>
                    </ul>
                </div>
            </td>
        </tr>
    </tbody>

</table>