<div class="row">
    <div class="col-xs-12">
        <div class="box box-solid">
            <div class="box-body">
                <div class="box-group" id="accordion">
                    <div class="panel box box-primary">
                        <table id="example1" class="table table-bordered table-condensed table-hover table-striped" role="grid" aria-describedby="example1_info">
                            <thead>
                                <tr style="font-size: 11px;">
                                    <th style="width:95px">Type d'affaire</th>
                                    <th style="width:80px">Audience du</th>
