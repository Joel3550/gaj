                                    <th style="width:120px">Greffier</th>
                                    <th rowspan="2" style="width:70px">Demandeur</th>
                                    <th rowspan="2" style="width:80px">Defendeur</th>
                                    <th rowspan="2" style="width:140px">Objet de la demande</th>
                                    <th rowspan="2" style="width:110px">Observation</th>
                                    <!--<th style="width:40px"></th>-->
                                </tr>
                            </thead>
                            <tbody>
                                <tr ng-repeat="liste in listeAffairesPlumitif | limitTo : limit" style="font-size: 11px" ng-if="liste.id_user_affaire == id_user">
                                    <td>{{liste.type_affaire}}</td>
                                    <td>{{liste.date_audience | date : 'dd/MM/yyyy'}}</td>
                                    <td>{{liste.president}}</td>
                                    <td>{{liste.assesseurs}}</td>
                                    <td>{{liste.substitut}}</td>
                                    <td>{{liste.greffier}}</td>
