                                            <!--&lt;!&ndash;</tr>&ndash;&gt;-->
                                            <!--<tr>-->
                                                <!--<td style="width:120px; border-right: 1px solid #f4f4f4">{{liste.president}}</td>-->
                                                <!--<td style="width:110px; border-right: 1px solid #f4f4f4">{{liste.assesseurs}}</td>-->
                                                <!--<td style="width:120px; border-right: 1px solid #f4f4f4">{{liste.substitut}}</td>-->
                                                <!--<td style="width:100px; border-right: 1px solid #f4f4f4">{{liste.greffier}}</td>-->
                                            <!--</tr>-->
                                        <!--</table>-->
                                    <!--</td>-->
                                    <td style="width: 120px">
                                        <div ng-repeat="partie in individusAJuger">
                                            <div ng-if="partie.id_affaire_individu == liste.id_affaire && partie.id_typeIndividu_individu == 1">
                                                {{partie.nom_individu}} {{partie.prenom_individu | limitTo : 8}}
                                            </div>
                                        </div>
                                    </td>
                                    <td style="width: 120px">
                                        <div ng-repeat="partie in individusAJuger">
                                            <div ng-if="partie.id_affaire_individu == liste.id_affaire && partie.id_typeIndividu_individu == 2">
                                                {{partie.nom_individu}} {{partie.prenom_individu | limitTo : 8}}
                                            </div>
                                        </div>
                                    </td>
                                    <td>{{liste.objet_affaire}}</td>
