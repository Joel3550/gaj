    <script>

      function printPublication() {
        var doc2 = new jsPDF('p', 'pt', 'a4');
        var specialElementHandlers = {
          '#bypassme': function (element, renderer) {
            return true;
          }
        };

        doc2.fromHTML(
                $('#testcase').html(),
                10, // x coord
                10, // y coord
                {
                  'width': 570, // max width of content on PDF
                  'elementHandlers': specialElementHandlers
                });
        doc2.output('dataurl');
      }
    </script>
